{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/Best/(.*)", "dest": "/public/Best/$1" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
git add vercel.json
git commit -m "Added static route config for images"
git push origin main
